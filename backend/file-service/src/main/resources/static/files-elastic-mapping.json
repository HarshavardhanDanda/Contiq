{
  "dynamic_templates": [
    {
      "strings_as_keywords": {
        "match_mapping_type": "string",
        "mapping": {
          "type": "keyword"
        }
      }
    }
  ],
  "properties": {
    "id": {
      "type": "keyword"
    },
    "name": {
      "type": "text"
    },
    "type": {
      "type": "text"
    },
    "content": {
      "type": "text"
    },
    "uploadedBy": {
      "type": "integer"
    },
    "createdAt": {
      "type": "date",
      "format": "strict_date_optional_time||epoch_millis"
    },
    "uploadedAt": {
      "type": "date",
      "format": "strict_date_optional_time||epoch_millis"
    }
  }
}